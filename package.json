{
  "name": "gleece-extension",
  "displayName": "Gleece",
  "description": "The Gleece extension for Visual Studio Code",
  "version": "0.0.3",
  "publisher": "haim-kastner",
  "author": {
    "name": "Haim Kastner",
    "email": "hello@haim-kastner.com"
  },
  "icon": "resources/logo.jpg",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "keywords": [
    "go",
    "golang",
    "annotation",
    "documentation",
    "gleece"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onLanguage:golang"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/gopher-fleece/gleece-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/gopher-fleece/gleece-vscode-extension/issues"
  },
  "homepage": "https://github.com/gopher-fleece/gleece-vscode-extension#readme",
  "scripts": {
    "compile": "webpack",
    "watch": "npm run compile && webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "vscode:prepublish": "npm run package",
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "lodash.debounce": "^4.0.8",
    "simple-git": "^3.22.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "go",
        "aliases": [
          "Go",
          "golang"
        ],
        "extensions": [
          ".go"
        ]
      }
    ]
  }
}
